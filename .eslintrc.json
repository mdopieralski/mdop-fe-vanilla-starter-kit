{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": { },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "html"
    ],
    "ignorePatterns": ["dist/**/*.*"],
    "rules": {
        "indent": ["error", 4],
        "max-len": ["error", { 
            "code": 160
        }],
        "no-useless-catch": ["off"],
        "import/prefer-default-export": ["off"]
    }
}